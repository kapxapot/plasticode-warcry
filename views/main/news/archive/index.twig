{% extends 'main/layout.twig' %}

{% import 'main/local_macros.twig' as lm %}

{% set no_disqus = true %}

{% block content %}
	<div class="panel panel-primary">
		{{ lm.panel_header(_context) }}
		{% if years %}
			<div class="panel-body">
				{% for y in years %}
					<a class="label label-default" href="{{ y.url }}">{{ y.title }}</a>
				{% endfor %}
			</div>
		{% else %}
			<div class="panel-body">
				Архив пуст.
			</div>
		{% endif %}
	</div>
{% endblock %}
